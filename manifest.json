{
  "manifest_version": 2,
  "name": "MultiNewTab",
  "description": "Use several New Tab extensions at once.",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
          "management",
          "storage"
        ],
  "background": {
  	"scripts": ["jquery-3.3.1.min.js", "background.js"]
  }
}