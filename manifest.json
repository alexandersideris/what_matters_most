{
  "manifest_version": 2,
  "name": "Hello World",
  "description": "Holy moly this is easy",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
          "management",
          "newTabPageOverride",
          "tabs"
        ],
  "background": {
  	"scripts": ["jquery-3.3.1.min.js", "background.js"]
  }
}