{
  "manifest_version": 2,
  "name": "multi new tab",
  "description": "Use multiple 'New Tab' chrome extensions simultaneously!",
  "version": "1.1.0",
  "browser_action": {
    "default_icon": "/assets/icon.png",
    "default_title": "Have multiple new tabs!",
    "default_popup": "popup.html"
  },
  "icons": { "64": "/assets/icon.png" },
  "permissions": ["management", "storage"],
  "background": {
    "scripts": ["background.js"]
  }
}
